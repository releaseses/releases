{
  "name": "releases",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "prerelease",
    "@fortawesome/free-solid-svg-icons": "prerelease",
    "@fortawesome/react-fontawesome": "prerelease",
    "autoprefixer": "7.1.6",
    "bulma": "^0.7.1",
    "bulma-timeline": "^3.0.0",
    "chalk": "1.1.3",
    "chroma-js": "^1.3.7",
    "classnames": "^2.2.6",
    "connected-react-router": "^4.3.0",
    "css-loader": "0.28.7",
    "expect": "^23.3.0",
    "fs-extra": "3.0.1",
    "moment": "^2.22.2",
    "normalizr": "^3.2.4",
    "npm": "^6.4.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-datepicker": "^1.5.0",
    "react-dom": "^16.4.1",
    "react-mde": "^5.7.0",
    "react-moment": "^0.7.9",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.0.0",
    "react-spinkit": "^3.0.0",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-forms-markdown-editor": "^1.1.6",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "resolve": "1.6.0",
    "showdown": "^1.8.6",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "test": "node scripts/test.js --env=jsdom",
    "build": "node_modules/.bin/webpack --config webpack.config.js",
    "coverage": "node_modules/.bin/nyc --reporter=lcov npm test",
    "heroku-postbuild": "npm run build"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "html-webpack-plugin": "2.29.0",
    "jest": "^23.4.1",
    "node-sass": "^4.9.2",
    "nyc": "^13.0.1",
    "react-dev-utils": "^5.0.1",
    "react-test-renderer": "^16.4.1",
    "sass-loader": "^7.0.3",
    "stats-webpack-plugin": "^0.6.2",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/scripts/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/apps/**/assets/javascripts/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/scripts/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/scripts/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
